{% extends 'base.html.twig' %}

{% block title%}Diary{% endblock %}

{% block body %}
  {% for diary in diaries %}
    <p>Date: {{ diary.diaryDate|date('Y-m-d') }}</p>
    <p>Weather: {{ diary.weather }}</p>
    <p>Author: {{ diary.author }}</p>
    <p>Content: {{ diary.content }}</p>
  {% else %}
    No diary.
  {% endfor %}
{% endblock %}

